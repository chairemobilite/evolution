  "scripts": {
    "test": "echo 'no test on this workspace'",
    "rollback": "knex migrate:rollback",
    "rollback-test": "cross-env NODE_ENV=test knex migrate:rollback",
    "seed": "cross-env NODE_ENV=test knex seed:run",
