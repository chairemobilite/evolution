{
    "engines": {
        "node": ">=18.0.0"
    },
    "name": "demo_survey",
    "version": "1.0.0",
    "main": "index.js",
    "author": "Chaire Mobilit√©",
    "private": true,
    "repository": "github.com/chairemobilite/evolution",
    "scripts": {
        "build:dev": "cross-env NODE_OPTIONS=\"--trace-deprecation --max_old_space_size=4096 --openssl-legacy-provider\" webpack watch --progress --color --node-env development --mode development",
        "build:prod": "cross-env NODE_OPTIONS=\"--trace-deprecation --max_old_space_size=4096 --openssl-legacy-provider\" webpack --progress --color --node-env production --mode production",
        "build:admin:dev": "cross-env NODE_OPTIONS=\"--trace-deprecation --max_old_space_size=4096 --openssl-legacy-provider\" webpack watch --config webpack.admin.config.js --progress --color --node-env development --mode development",
        "build:admin:prod": "cross-env NODE_OPTIONS=\"--trace-deprecation --max_old_space_size=4096 --openssl-legacy-provider\" webpack --config webpack.admin.config.js --progress --color --node-env production --mode production",
        "clean": "rimraf lib/",
        "cleanModules": "rimraf node_modules/",
        "copy-files": "copyfiles -u 1 src/survey/*.json src/survey/*.geojson src/styles/*.scss ./lib/",
        "compile": "tsc && yarn copy-files",
        "compile:dev": "yarn copy-files && tsc -w",
        "start": "yarn babel-node lib/server.js --max-old-space-size=4096",
        "start:admin": "yarn babel-node lib/admin/server.js --max-old-space-size=4096",
        "start:tracing": "yarn babel-node -r ../../tracing.js lib/server.js --max-old-space-size=4096",
        "start:admin:tracing": "yarn babel-node -r ../../tracing.js lib/admin/server.js --max-old-space-size=4096",
        "start:debug": "cross-env DEBUG=express:* yarn babel-node --trace-warnings lib/server.js --max-old-space-size=4096",
        "test": "cross-env NODE_ENV=test jest --config=jest.config.js",
        "test:survey": "LOCALE_DIR=$(pwd)/locales npx playwright test"
    },
    "dependencies": {
        "@fortawesome/free-solid-svg-icons": "^5.15.1",
        "@fortawesome/react-fontawesome": "^0.1.11",
        "@react-google-maps/api": "^2.12.1",
        "@turf/turf": "^6.3.0",
        "chaire-lib-backend": "0.2.2",
        "chaire-lib-common": "0.2.2",
        "chaire-lib-frontend": "0.2.2",
        "chalk": "^4.1.0",
        "cross-env": "^7.0.2",
        "csv-parse": "^4.11.1",
        "evolution-backend": "0.2.2",
        "evolution-common": "0.2.2",
        "evolution-frontend": "0.2.2",
        "evolution-legacy": "0.2.2",
        "geojson-validation": "^1.0.2",
        "history": "^4.9.0",
        "json2csv": "^5.0.7",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "moment-business-days": "^1.2.0",
        "moment-timezone": "^0.5.35",
        "react": "^16.13.1",
        "react-highcharts": "^16.1.0",
        "react-i18next": "^12.2.2",
        "slugify": "^1.6.5",
        "typescript": "^4.9.4",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@alienfast/i18next-loader": "^2.0.2",
        "@types/geojson": "^7946.0.7",
        "@types/lodash": "^4.14.198",
        "@types/node": "^17.0.38",
        "@types/react": "^16.9.0",
        "@types/react-test-renderer": "^16.9.2",
        "babel-loader": "^8.1.0",
        "bundle-loader": "^0.5.6",
        "clean-webpack-plugin": "^4.0.0",
        "compression-webpack-plugin": "^11.1.0",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^6.10.0",
        "html-webpack-plugin": "^5.6.0",
        "jest": "^29.7.0",
        "jest-each": "^29.7.0",
        "json-loader": "^0.5.7",
        "mini-css-extract-plugin": "^2.9.0",
        "sass-loader": "^14.2.1",
        "source-map-loader": "^5.0.0",
        "ts-loader": "^9.5.1",
        "ts-shader-loader": "^2.0.2",
        "webpack": "^5.92.0",
        "webpack-cli": "^5.1.4"
    }
}
